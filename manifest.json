{
    "name": "Fast generate URL-QR",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "description": "Generate QR code from url of active tab",
    "browser_action": {
       "default_icon": "img/icon.png",
       "default_title": "Fast generate URL-QR",
       "default_popup":"html/popup.html"
    },
    "icons": {
        "48": "img/icon.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "img/icon.png"
    ]
}